<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android" 
    android:shape="rectangle" >

    <!-- 定义几何图形的四个角的弧度 -->
    <corners
        android:radius="3dp" />

    <!-- 定义填充颜色 -->
    <solid
        android:color="#66696969" />

    <!-- 定义边框 -->
    <stroke
        android:width="2dp"
        android:color="#66696969" />

    <!-- 定义几何图形大小 -->
    <size
        android:width="15dp"
        android:height="28dp"
        />
    
</shape>
