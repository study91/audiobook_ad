<?xml version="1.0" encoding="utf-8"?>

<!-- 课文内容按钮 -->
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <!-- 按钮按下时的图片 -->
    <item
        android:state_pressed="true"
        android:drawable="@mipmap/catalog_child_explain_pressed"/>
    <!-- 按钮有效时的图片 -->
    <item
        android:state_enabled="true"
        android:drawable="@mipmap/catalog_child_explain_normal"/>
    <!-- 按钮失效时的图片 -->
    <item
        android:state_enabled="false"
        android:drawable="@mipmap/catalog_child_explain_invalid"/>
</selector>